{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://portfolio-theta-smoky-22.vercel.app"
      }
    }
  ]
}
